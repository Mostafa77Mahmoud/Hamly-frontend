{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 npx expo start",
    "dev:web": "cross-env EXPO_NO_TELEMETRY=1 HOST=0.0.0.0 npx expo start --web --port 5000",
    "build": "cross-env EXPO_NO_TELEMETRY=1 npx expo export --platform web --output-dir dist",
  "build:web": "npx expo export --platform web --output-dir dist --no-ssr",
    "start": "cross-env EXPO_NO_TELEMETRY=1 npx expo start --web --port 5000 --offline",
    "serve": "npx serve -s dist -l 5000",
    "lint": "npx expo lint",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.57.4",
    "@types/node": "^24.4.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "expo": "~52.0.0",
    "expo-av": "~15.0.2",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-device": "~7.0.3",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "lucide-react-native": "^0.544.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.1.0",
    "serve": "^14.2.5",
    "tsx": "^4.20.5",
    "typescript": "~5.3.3"
  }
}
